/var/log/qingcloud-flex-volume/*.log.* {
    rotate 1
    missingok
    notifempty
    daily
    compress
    maxsize 20M
    create 0644 root root
    postrotate
       find /var/log/qingcloud-flex-volume/ -name "*.1" -mtime +7 -delete
    endscript
}

/var/log/qingcloud-flex-volume-controller-manager/*.log.* {
    rotate 1
    missingok
    notifempty
    daily
    compress
    maxsize 20M
    create 0644 root root
    postrotate
       find /var/log/qingcloud-flex-volume-controller-manager/ -name "*.1" -mtime +7 -delete
    endscript
}
