/var/log/qingcloud-flex-volume/*.log.* {
    firstaction
        /opt/kubernetes/script/compressFlexLog.sh
    endscript
    rotate 0
    missingok
    ifempty
    daily
}

/var/log/qingcloud-flex-volume-controller-manager/*.log.* {
    firstaction
        /opt/kubernetes/script/compressFlexMgrLog.sh
    endscript
    rotate 0
    missingok
    ifempty
    daily
}